    "chai-as-promised": "^5.1.0",
    "connect-history-api-fallback": "^1.1.0",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.23.0",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.0.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.3.1",
