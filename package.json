{
  "name": "onigiri",
  "version": "0.0.1",
  "description": "Better workflow for developer.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint ./src/**/*.js",
    "lint:tests": "eslint ./tests/**/*.spec.js",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "start": "npm run dev",
    "dev": "node -r dotenv/config --harmony bin/server",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "node -r dotenv/config ./node_modules/karma/bin/karma start bin/karma.js",
    "test:dev": "npm run test -- --watch",
    "deploy": "npm run lint && npm run test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaoyuan-mountain/onigiri.git"
  },
  "author": "GaoYuan",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.23",
    "debug": "^2.2.0",
    "dotenv": "^1.2.0",
    "es6-promise": "^3.0.2",
    "history": "^1.9.0",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0",
    "redux": "^3.0.0",
    "redux-history-transitions": "^1.0.0",
    "redux-simple-router": "^0.0.10",
    "redux-thunk": "^1.0.0",
    "yargs": "^3.18.0"
  },
